{
  "name": "Ubbi Dubbi",
  "description": "Translate web pages into Ubbi Dubbi",
  "version": "1.0",
  "manifest_version": 3,
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon" : {
      "16": "images/zoom16.jpeg",
      "32": "images/zoom32.jpeg",
      "48": "images/zoom48.jpeg",
      "128": "images/zoom128.jpeg"
  }
},
  "icons": {
    "16": "/images/zoom16.jpeg",
    "32": "/images/zoom32.jpeg",
    "48": "/images/zoom48.jpeg",
    "128": "/images/zoom128.jpeg"
  }
}

