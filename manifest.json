{
  "name": "Ubbi Dubbi",
  "description": "Translate web pages into Ubbi Dubbi",
  "version": "1.0",
  "manifest_version": 3,
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
    
  },
  "icons": {
    "16": "zoom64.png",
    "32": "zoom64.png",
    "48": "zoom64.png",
    "128": "zoom64.png"
  }
}

